    1  mkdir Downloads
    2  cd Downloads/
    3  wget https://repo.anaconda.com/archive/Anaconda3-5.1.0-Linux-x86_64.sh
    4  bash Anaconda3-5.1.0-Linux-x86_64.sh
    5  sudo add-apt-repository ppa:deadsnakes/ppa
    6  sudo apt-get update
    7  sudo apt-get install python3.6
    8  export PATH=~/anaconda3/bin:$PATH
    9  conda update conda
   10  conda create -n py36 python=3.6 anaconda
   11  python -m pip install pyspark
   12  sudo apt-get install default-jre
   13  sudo apt-get install default-jdk
   14  export JAVA_HOME=/usr/lib/jvm/java-8-openjdk-amd64
   15  cd ..
   16  git clone https://github.com/aitsaadamira/bdd.git
   17  cd bdd
   18  python learning_models_ml.py 
   19  echo "`history`" > history
